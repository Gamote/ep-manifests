{
  "id": "com.envpilot.plugin.homebrew",
  "name": "homebrew",
  "version": "0.0.1",
  "capabilities": {
    "apps": {
      "checkInstallation": [
        "brew list --cask \"${{ app.id }}\""
      ],
      "checkVersion": [
        "brew list --version --cask \"${{ app.id }}\" | awk '{print $2}'"
      ]
    }
  }
}
